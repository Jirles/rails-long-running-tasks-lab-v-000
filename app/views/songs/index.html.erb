<%= form_for(:song, url: songs_upload_path, method: 'post') do |f| %>
  <%= f.file_field :csv %><br>
  <%= f.submit 'Upload' %><br>
<% end %>

<ul>
<% @songs.each do |song| %>
  <li><%= display_artist(song) %> - <%= link_to song.title, song_path(song) %></li>
<% end %>
</ul>
<br>
<%= link_to "Add New Song", new_song_path %>
